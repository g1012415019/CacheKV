{
    "name": "asfop/cache-kv",
    "description": "简化缓存操作的 PHP 库，专注于实现若无则从数据源获取并回填缓存这一常见模式",
    "version": "1.0.0",
    "type": "library",
    "keywords": ["cache", "redis", "php", "kv", "key-value", "hot-key", "statistics", "performance"],
    "license": "MIT",
    "homepage": "https://github.com/asfop/cache-kv",
    "authors": [
        {
            "name": "ASFOP",
            "email": "support@asfop.com"
        }
    ],
    "require": {
        "php": ">=7.0",
        "ext-redis": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Asfop\\CacheKV\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Asfop\\CacheKV\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "support": {
        "issues": "https://github.com/asfop/cache-kv/issues",
        "source": "https://github.com/asfop/cache-kv"
    }
}
